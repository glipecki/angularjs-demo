{
    "compileOnSave": false,
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "declaration": false,
        "target": "es5",
        "module": "system",
        "emitDecoratorMetadata": true,
        "inlineSourceMap": false,
        "outDir": "../../../target/ts-compiled/",
        "sourceRoot": "/",
        "rootDir": "./src/main/ts"
    },
    "filesGlob": [
        "**/*.ts",
        "../../../typings/**/*.d.ts"
    ],
    "files": [
        "app.ts",
        "dashboard/DashboardController.ts",
        "dashboard/DashboardModule.ts",
        "dashboard/DashboardRouting.ts",
        "dashboard/IDashboardController.ts",
        "index.ts",
        "lazy/LazySystemService.ts",
        "login/ILoginController.ts",
        "login/LoginController.ts",
        "login/LoginModule.ts",
        "login/LoginRouting.ts",
        "systemjs-config.ts",
        "tasks/ITasksController.ts",
        "tasks/TasksController.ts",
        "tasks/TasksModule.ts",
        "tasks/TasksRouting.ts",
        "../../../typings/angular-protractor/angular-protractor.d.ts",
        "../../../typings/angular-ui-router/angular-ui-router.d.ts",
        "../../../typings/angularjs/angular-mocks.d.ts",
        "../../../typings/angularjs/angular.d.ts",
        "../../../typings/jasmine/jasmine.d.ts",
        "../../../typings/jquery/jquery.d.ts",
        "../../../typings/oclazyload/oclazyload.d.ts",
        "../../../typings/selenium-webdriver/selenium-webdriver.d.ts",
        "../../../typings/systemjs/systemjs.d.ts",
        "../../../typings/tsd.d.ts"
    ],
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4,
        "newLineCharacter": "\r\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": false,
        "placeOpenBraceOnNewLineForControlBlocks": false
    }
}
